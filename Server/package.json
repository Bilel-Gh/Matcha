{
  "name": "matcha-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "db:reset": "docker compose down -v && docker compose up -d database",
    "db:status": "docker compose exec database psql -U matcha_user -d matcha -c 'SELECT * FROM schema_migrations ORDER BY applied_at;'",
    "db:create-migration": "node scripts/create-migration.js",
    "db:migrate": "docker compose exec database psql -U matcha_user -d matcha -f /docker-entrypoint-initdb.d/init.sql",
    "db:seed": "docker compose exec database psql -U matcha_user -d matcha -f /docker-entrypoint-initdb.d/seeds/development_seeds.sql",
    "create-test-images": "node create-test-images.js",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "nodemailer": "^6.9.14"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
